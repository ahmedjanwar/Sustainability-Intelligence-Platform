{
  "version": 2,
  "builds": [
    {
      "src": "api/simple_index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/simple_index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  }
}
